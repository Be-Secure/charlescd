								"type": "text/javascript",
								"exec": [
									"var uuid = require('uuid');",
									"",
									"postman.setGlobalVariable('moduleRequestUUID', uuid.v4());",
									"postman.setGlobalVariable('componentRequestUUID', uuid.v4());",
									"postman.setGlobalVariable('authorRequestUUID', uuid.v4());"
								]
